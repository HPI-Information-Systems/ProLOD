# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


# Home page
GET        /                   controllers.Application.index()

GET        /datasets           controllers.Datasets.datasets()

GET        /persons            controllers.Table1.getAll()
GET        /persons/:id        controllers.Table1.getPerson(id : Int)
GET        /personslink        controllers.Table1.getPersonLinks

# Map the JS resource paths
GET        /*file              controllers.Assets.versioned(path="/public", file: Asset)

